-- .env.psql

-- App / database
\set APP_DB myapp

-- Roles (fully-formed identifiers)
\set MIGRATOR_ROLE myapp_migrator
\set AUTHENTICATOR_ROLE myapp_authenticator
\set ANON_ROLE myapp_anon
\set SERVICE_ROLE myapp_service
\set CLIENT_ROLE myapp_client

-- Secrets - generate with openssl rand -hex 16 |tr -d '\n'
\set MIGRATOR_PASS '''pass'''
\set PGRST_AUTHENTICATOR_PASS '''pass'''
